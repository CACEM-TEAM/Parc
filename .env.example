# ========================================
# Configuration ParcAuto - Variables d'environnement
# Copiez ce fichier vers .env et modifiez les valeurs selon vos besoins
# ========================================

# Configuration du serveur
NODE_ENV=development
PORT=4500

# Configuration de la base de donn√©es MySQL
DB_HOST=mysql
DB_USER=parcauto_user
DB_PASSWORD=parcauto_password
DB_NAME=parcauto
DB_PORT=3306

# Configuration MySQL (Docker)
MYSQL_ROOT_PASSWORD=rootpassword
MYSQL_DATABASE=parcauto
MYSQL_USER=parcauto_user
MYSQL_PASSWORD=parcauto_password

# Configuration JWT
JWT_SECRET=votre_secret_jwt_tres_securise_production
JWT_EXPIRES_IN=24h

# Configuration CORS
CORS_ORIGIN=http://localhost:3000

# Configuration Frontend
VITE_API_URL=http://localhost:4500

# Configuration phpMyAdmin
PMA_HOST=mysql
PMA_PORT=3306
PMA_USER=root
PMA_PASSWORD=rootpassword

# Configuration des ports
FRONTEND_PORT=3000
BACKEND_PORT=4500
MYSQL_PORT=3306
PHPMYADMIN_PORT=8080 